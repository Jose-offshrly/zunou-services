{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "auth0/auth0-php": "^8.11",
        "auth0/login": "^7.9",
        "aws/aws-sdk-php": "^3.300",
        "fakerphp/faker": "^1.23",
        "google/apiclient": "^2.15.0",
        "google/apiclient-services": "^0.300.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel-notification-channels/webpush": "^10.2",
        "laravel/framework": "^11.23.5",
        "laravel/sanctum": "^4.0.2",
        "laravel/tinker": "^2.9.0",
        "laravel/vapor-cli": "^1.63",
        "laravel/vapor-core": "^2.37.1",
        "league/commonmark": "^2.5",
        "mll-lab/laravel-graphiql": "^3.1",
        "nuwave/lighthouse": "^6.30",
        "openai-php/client": "^0.10.1",
        "predis/predis": "^3.3",
        "probots-io/pinecone-php": "^1.0.1",
        "pusher/pusher-php-server": "^7.2",
        "pusher/pusher-push-notifications": "^2.0",
        "ramsey/uuid": "^4.7",
        "sentry/sentry-laravel": "^4.15",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-google-calendar": "^3.8",
        "stripe/stripe-php": "^15.7",
        "tightenco/parental": "^1.3",
        "voku/portable-ascii": "^2.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "beyondcode/laravel-query-detector": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.51",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.4.0",
        "overtrue/phplint": "^9.4",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "google/apiclient-services": [
            "Iam",
            "Sheets",
            "Calendar"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "bamarni/composer-bin-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
